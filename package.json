{
  "name": "gql",
  "version": "0.0.1",
  "description": "Awesome project developed with TypeORM.",
  "devDependencies": {
    "@types/bcrypt": "^3.0.0",
    "@types/bcryptjs": "^2.4.2",
    "@types/jest": "^23.3.12",
    "@types/node": "^9.6.6",
    "@types/uuid": "^3.4.4",
    "@types/yup": "^0.26.4",
    "cross-env": "^5.2.0",
    "gql2ts": "^1.10.1",
    "jest": "^23.6.0",
    "nodemon": "^1.18.9",
    "ts-jest": "^23.10.5",
    "ts-node": "6.0.0",
    "tslint": "^5.12.0",
    "tslint-config-prettier": "^1.17.0",
    "typescript": "2.8.3"
  },
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "graphql": "^14.0.2",
    "graphql-import": "^0.7.1",
    "graphql-request": "^1.8.2",
    "graphql-tools": "^4.0.3",
    "graphql-yoga": "^1.16.9",
    "pg": "^7.4.1",
    "reflect-metadata": "^0.1.12",
    "typeorm": "0.2.11",
    "uuid": "^3.3.2",
    "yup": "^0.26.6"
  },
  "scripts": {
    "start": "cross-env NODE_ENV=development nodemon --exec ts-node src/index.ts",
    "test": "cross-env NODE_ENV=test jest --forceExit --detectOpenHandles",
    "gen-schema-types": "gql2ts src/schema.graphql -o src/types/schema.d.ts"
  },
  "jest": {
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  }
}
